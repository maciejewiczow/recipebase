{
  "name": "recipebase",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/react",
      "**/react-dom",
      "**/react-native",
      "**/react-native/**",
      "**/react-native*",
      "**/react-native*/**",
      "**/babel-plugin-transform-typescript-metadata",
      "**/babel-plugin-transform-typescript-metadata/**"
    ]
  },
  "scripts": {
    "reset": "wsl find . -type dir -name node_modules | xargs rm -rf && rm -rf yarn.lock",
    "test": "jest"
  },
  "dependencies": {
    "jest": "^25.5.1",
    "ts-jest": "^25.1.4",
    "typescript": "^4.7.3"
  }
}
